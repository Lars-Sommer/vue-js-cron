    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Semantic Release --dry-run",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/multi-semantic-release/bin/cli.js",
            "args": [
                "--dry-run",
                "--ignore-private-packages"
            ],
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}